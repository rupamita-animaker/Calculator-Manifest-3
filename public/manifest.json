{
  "name": "Calculator Manifest 3",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts" : [
    {
      "matches": [ "<all_urls>" ],
      "run_at": "document_idle",
      "css": ["/static/css/app.css"],
      "js": ["/static/js/content.js"]
    }
  ],
    "permissions": [
        "tabs",
        "scripting",
        "storage",
        "contextMenus"
    ],
    "host_permissions": [
      "<all_urls>"
    ]
}